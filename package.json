{
    "name": "mk-react-comp",
    "version": "1.0.6",
    "description": "React CLI allowing you to create react components from the command line with lots of customizations.",
    "author": "Jimmy Beldone <dev.jimmy.beldone@gmail.com>",
    "keywords": [
        "react",
        "cli",
        "component",
        "generator"
    ],
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/JimmyBeldone/mk-react-comp"
    },
    "scripts": {
        "lint": "eslint src",
        "test": "babel-node src/index.js Test --styl -f -l -r",
        "run:help": "babel-node src/index.js -h",
        "build": "yarn clean && babel src --out-dir lib",
        "clean": "rimraf lib",
        "clean:test": "rimraf src/views"
    },
    "preferGlobal": true,
    "bin": {
        "mkreact": "lib/index.js"
    },
    "devDependencies": {
        "@babel/cli": "^7.0.0",
        "@babel/core": "^7.0.0",
        "@babel/node": "^7.0.0",
        "@babel/preset-env": "^7.0.0",
        "babel-eslint": "^10.0.1",
        "babel-plugin-array-includes": "^2.0.3",
        "chalk": "^2.4.1",
        "commander": "^2.19.0",
        "eslint": "^5.8.0",
        "eslint-config-standard": "^12.0.0",
        "eslint-plugin-import": "^2.14.0",
        "eslint-plugin-node": "^8.0.0",
        "eslint-plugin-promise": "^4.0.1",
        "eslint-plugin-standard": "^4.0.0",
        "rimraf": "^2.5.2"
    }
}

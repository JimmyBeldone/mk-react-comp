{
    "extends": "standard",
    "parser": "babel-eslint",
    "env": {
        "node": true
    },
    "rules": {
        "quotes": 0,
        "no-console": 0,
        "no-debugger": 1,
        "no-var": 1,
        "indent": 0,
        "semi": 0,
        "no-trailing-spaces": 0,
        "eol-last": 0,
        "no-underscore-dangle": 0,
        "no-alert": 0,
        "no-lone-blocks": 0,
        "padded-blocks": 0,
        "space-before-function-paren": 0,
        "no-unused-vars": 0
    }
}
